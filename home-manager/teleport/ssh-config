# Common flags for all teleport.cimbulka.net hostName
Host *.teleport.cimbulka.net
    UserKnownHostsFile "/home/matyas/.tsh/known_hosts"
    IdentityFile "/home/matyas/.tsh/keys/teleport.cimbulka.net/matyas"
    CertificateFile "/home/matyas/.tsh/keys/teleport.cimbulka.net/matyas-ssh/teleport.cimbulka.net-cert.pub"
    HostKeyAlgorithms rsa-sha2-512-cert-v01@openssh.com,rsa-sha2-256-cert-v01@openssh.com,ssh-rsa-cert-v01@openssh.com

# Flags for all teleport.cimbulka.net hosts except the proxy
Host *.teleport.cimbulka.net !teleport.cimbulka.net
    Port 3025
    ProxyCommand "/home/matyas/.nix-profile/bin/tsh" proxy ssh --cluster=teleport.cimbulka.net --proxy=teleport.cimbulka.net:443 %r@%h:%p